    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="phonegap-facebook-plugin" spec="0.12.0">
        <variable name="APP_ID" value="148235158916239" />
        <variable name="APP_NAME" value="ShevetApp" />
    </plugin>
    <plugin name="cordova-plugin-calendar" source="npm" version="4.4.2" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="http://app.shevetahim.com/*" />
    <access launch-external="yes" origin="tel:*" />
